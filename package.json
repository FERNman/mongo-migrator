{
  "name": "mongo-migrator",
  "version": "0.1.0",
  "description": "A database migration tool for MongoDB.",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "ts-node bin/mongo-migrator.ts"
  },
  "bin": "dist/bin/mongo-migrator",
  "keywords": [
    "mongodb",
    "database",
    "migration",
    "typescript"
  ],
  "author": "Gabriel Sperrer",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,html,scss,css,yml,md}": "prettier --write",
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "glob": "^7.1.6",
    "mongodb": "^3.6.2",
    "yargs": "^16.0.3"
  },
  "peerDependencies": {
    "mongodb": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.13",
    "@types/mongodb": "^3.5.27",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "mongodb-memory-server": "^6.7.4",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
